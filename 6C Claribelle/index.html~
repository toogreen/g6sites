<script type="text/javascript">
var myVideo=document.getElementById("video1");
cc=0;
function changebutton1()
{
if (cc==0) 
  {
  cc=1;
  document.getElementById('button1').src="button1-on.gif";
  myVideo.play();
  }
else
  {
  cc=0;
  document.getElementById('button1').src="button1-off.gif";
  myVideo.pause();
  }
}
function changebutton2()
{ 
if (cc==0) 
  {
  cc=1;
  document.getElementById('button2').src="button2-on.gif";
  myVideo.height=(myVideo.videoHeight/4);
  }
else
  {
  cc=0;
  document.getElementById('button2').src="button2-off.gif";
  myVideo.height=(myVideo.videoHeight);
  }
}
function changebutton3()
{
if (cc==0) 
  {
  cc=1;
  document.getElementById('button3').src="button3-on.gif";
  myVideo.height=(myVideo.videoHeight);
  }
else
  {
  cc=0;
  document.getElementById('button3').src="button3-off.gif";
  myVideo.height=(myVideo.videoHeight);
  }
}
function changebutton4()
{
if (cc==0) 
  {
  cc=1;
  document.getElementById('button4').src="button4-on.gif";
   myVideo.height=(myVideo.videoHeight*3);
  }
else
  {
  cc=0;
  document.getElementById('button4').src="button4-off.gif";
  myVideo.height=(myVideo.videoHeight);
  }
}
</script> 

