<script type="text/javascript">
function changebutton1()
{
if (cc==0) 
  {
  cc=1;
  document.getElementById('button1').src="charactersbutton.jpg";
myVideo.play();
  }
else
  {
  cc=0;
  document.getElementById('button1').src="charactersbutton.jpg";
 myVideo.pause();
 }
}

